{
  "name": "ai-bug-reproduction-tool",
  "version": "0.1.0",
  "description": "AI-powered bug reproduction tool that converts natural language bug reports into deterministic, runnable repros",
  "private": true,
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "test": "turbo test",
    "test:ci": "turbo test --filter=!./apps/frontend",
    "clean": "turbo clean && rm -rf node_modules",
    "typecheck": "turbo typecheck",
    "docker:build": "docker build -f docker/Dockerfile .",
    "db:migrate": "cd infra && pnpm migrate",
    "db:seed": "cd infra && pnpm seed"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "turbo": "^1.11.0",
    "typescript": "^5.3.0",
    "prettier": "^3.1.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  }
}

